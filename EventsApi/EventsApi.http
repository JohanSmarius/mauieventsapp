@EventsApi_HostAddress = http://localhost:5144

# Get all events
GET {{EventsApi_HostAddress}}/api/events
Accept: application/json

###

# Get specific event by ID
GET {{EventsApi_HostAddress}}/api/events/1
Accept: application/json

###

# Get another specific event by ID
GET {{EventsApi_HostAddress}}/api/events/2
Accept: application/json

###

# Test non-existent event (should return 404)
GET {{EventsApi_HostAddress}}/api/events/999
Accept: application/json
